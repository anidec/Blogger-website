<%- include('partials/header') -%>
    <h1>Compose</h1>
    <form action="/compose" method="post">
        <div class="form-group">
            <label>Title</label>
            <input type="text" class="form-control" name="postTitle" required>
            <label>Post</label>
            <textarea name="postBody" class="form-control" cols="30" rows="5" required></textarea>
        </div>
        <button type="submit" class="btn btn-primary" name="button">Publish</button>
        <a class="btn btn-primary" href="/logout" role="button">Log Out</a>
    </form>
    <!-- <%- include('partials/footer') -%> -->